{
  "manifest_version": 3,
  "name": "Mycourseville Dark Theme",
  "version": "1.0",
  "description": "Simple dark theme for Mycourseville.",
  "homepage_url": "https://github.com/cu-devclub/MCV-dark-theme",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.mycourseville.com/*"],
      "css": ["style.css"]
    }
  ]
}
